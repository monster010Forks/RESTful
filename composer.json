{
  "name": "jason-napolitano/restful",
  "type": "library",
  "description": "A REST based package for CodeIgniter 4!",
  "homepage": "https://github.com/jason-napolitano/RESTful",
  "license": "MIT",
  "require": {
    "php": ">=7.2"
  },
  "suggest": {
    "monolog/monolog": "2.0.*"
  },
  "autoload": {
    "psr-4": {
      "RESTful\\": "./src"
    }
  },
  "config": {
    "optimize-autoloader": true
  },
  "scripts": {
    "post-install-cmd": "composer install monolog/monolog"
  }
}
